<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/AllNotesFragment">

    <fragment
        android:id="@+id/AllNotesFragment"
        android:name="com.example.noteplus.ui.all.AllNotesFragment"
        android:label="@string/all_notes_fragment_label"
        tools:layout="@layout/fragment_all_notes">
        <action
            android:id="@+id/action_AllNotesFragment_to_noteFragment"
            app:destination="@id/noteFragment" />
    </fragment>
    <fragment
        android:id="@+id/FoldersFragment"
        android:name="com.example.noteplus.ui.all_folders.FoldersFragment"
        android:label="@string/folders_fragment_label"
        tools:layout="@layout/fragment_folders">
    </fragment>
    <fragment
        android:id="@+id/noteFragment"
        android:name="com.example.noteplus.ui.note.NoteFragment"
        android:label="note_fragment"
        tools:layout="@layout/note_fragment" />
</navigation>